{
  "name": "@api-helper/cli",
  "version": "1.0.5",
  "description": "根据 Swagger 的接口定义生成 TypeScript/JavaScript 的接口类型及其请求函数代码。",
  "bin": {
    "apih": "bin/index.js"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "bin",
    "lib",
    "README.md"
  ],
  "engines": {
    "node": ">=13.0.0"
  },
  "scripts": {
    "build": "jest && npm run lint && npm run clean && npm run build:node",
    "build:node": "tsc --project ./tsconfig.node.json",
    "clean": "shx rm -rf bin && shx rm -rf lib",
    "lint": "eslint src/**/*.ts",
    "cwt": "concurrently -r -p '' -k 'npm run watch' 'npm run watch:test'",
    "test": "npm run pre-test && jest",
    "test:only": "jest",
    "test:update": "npm run test:only -- -u",
    "pre-test": "npm run clean && npm run build:es",
    "watch": "tsc -w",
    "watch:test": "jest -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ztz2/api-helper.git"
  },
  "keywords": [
    "validator",
    "validator-util",
    "validation",
    "validate",
    "assert"
  ],
  "author": {
    "name": "ztz2",
    "email": "448941273@qq.com",
    "url": "https://github.com/ztz2"
  },
  "bugs": {
    "url": "https://github.com/ztz2/api-helper/issues"
  },
  "homepage": "https://github.com/ztz2/api-helper#readme",
  "dependencies": {
    "@api-helper/core": "workspace:^",
    "@babel/runtime": "^7.13.10",
    "@types/json-schema": "^7.0.9",
    "axios": "^1.3.2",
    "cac": "^6.7.14",
    "consola": "^2.3.0",
    "fs-extra": "^7.0.1",
    "https-browserify": "^1.0.0",
    "node-fetch": "^2",
    "ora": "^3.4.0",
    "prompts": "^2.4.2",
    "shx": "^0.3.4",
    "stream-http": "^3.2.0",
    "ts-node": "^8.0.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "29.2.4",
    "@types/node": "^10.12.10",
    "@types/node-fetch": "^2.6.2",
    "@types/prompts": "^1.2.0",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^29.4.3",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "eslint": "^7.32.0",
    "jest": "^29.4.3",
    "json-schema": "^0.4.0",
    "lint-staged": "^10",
    "openapi-types": "^7.0.1",
    "shx": "^0.3.4",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "tsify": "^5.0.4",
    "tslib": "^2.5.0",
    "typescript": "^4.5.5",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
