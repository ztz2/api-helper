{
  "name": "@api-helper/cli",
  "version": "0.2.2",
  "description": "根据 Swagger 的接口定义生成 TypeScript/JavaScript 的接口类型及其请求函数代码。",
  "keywords": [
    "yapi",
    "codegen",
    "typescript"
  ],
  "homepage": "https://github.com/ztz2/api-helper",
  "bugs": {
    "url": "https://github.com/ztz2/api-helper/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ztz2/api-helper.git"
  },
  "license": "MIT",
  "author": {
    "name": "ztz2",
    "email": "448941273@qq.com",
    "url": "https://github.com/ztz2"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "apih": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "jest && npm run lint && npm run clean && npm run build:server",
    "build2": "npm run clean && npm run build:server",
    "build:server": "tsc -d",
    "lint": "eslint src/*.ts tests/*.ts",
    "clean": "shx rm -rf dist && mkdir dist",
    "test": "jest --coverage",
    "testApi": "ts-node -T src/index.ts",
    "testApiInit": "ts-node -T src/index.ts init",
    "testApiHelper": "ts-node -T src/index.ts help",
    "testOnly": "jest",
    "testUpdateSnapshot": "jest --coverage -u"
  },
  "lint-staged": {
    "*.{css,less,scss,sass,html,htm,vue,yml,yaml,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@api-helper/core": "workspace:^",
    "@babel/runtime": "^7.13.10",
    "@types/json-schema": "^7.0.9",
    "axios": "^1.3.2",
    "consola": "^2.3.0",
    "fs-extra": "^7.0.1",
    "node-fetch": "^2",
    "ora": "^3.4.0",
    "prompts": "^2.4.2",
    "shx": "^0.3.4",
    "ts-node": "^8.0.2",
    "typescript": "^4.2.3",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "29.2.4",
    "@types/node": "^10.12.10",
    "@types/node-fetch": "^2.6.2",
    "@types/prompts": "^1.2.0",
    "@types/yargs": "^16.0.1",
    "eslint": "^7.32.0",
    "jest": "^29.4.3",
    "json-schema": "^0.4.0",
    "lint-staged": "^10",
    "openapi-types": "^7.0.1",
    "ts-jest": "29.0.3"
  },
  "engines": {
    "node": ">=10.19.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
